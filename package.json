{
    "name": "emacs-functions",
    "displayName": "emacs-functions",
    "description": "Emacs basic functions",
    "version": "0.0.1",
    "publisher": "rrudi",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Keymaps"
    ],
    "keywords": [
        "emacs"
    ],
    "activationEvents": [
        "onCommand:emacs.set-mark-command"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "emacs.set-mark-command",
            "title": "set mark"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "homepage": "https://github.com/shirou/emacs-functions/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/shirou/emacs-functions.git"
	},
    "preview": true,
}
